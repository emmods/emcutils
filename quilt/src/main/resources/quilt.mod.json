{
	"schema_version": 1,
	"quilt_loader": {
		"group": "coffee.waffle",
		"id": "emcutils",
		"version": "${version}",
		"metadata": {
			"name": "Empire Minecraft Utilities",
			"description": "A collection of various utilities for the Empire Minecraft server.",
			"contributors": {
				"triphora": "Owner",
				"MrFrydae": "Former Owner",
				"BlockHead_56": "Contributor",
				"BasiqueEvangelist": "Contributor"
			},
			"contact": {
				"homepage": "https://emc.gs/t/84930",
				"sources": "https://github.com/emmods/emcutils",
				"issues": "https://github.com/emmods/emcutils/issues"
			},
			"license": "MIT",
			"icon": "emcutils.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"client_init": "coffee.waffle.emcutils.fabric.EMCUtilsQuilt",
			"client_events": "coffee.waffle.emcutils.fabric.EMCUtilsQuilt"
		},
		"depends": [
			{
				"id": "midnightlib",
				"versions": ">=1.4.1"
			},
			{
				"id": "minecraft",
				"versions": ">=1.20.1"
			},
			{
				"id": "modmenu",
				"versions": ">=7.1.0",
				"optional": true
			}
		]
	},
	"minecraft": {
		"environment": "client"
	},
	"mixin": "emcutils-common.mixins.json",
	"access_widener": "emcutils.accesswidener",
	"modmenu": {
		"links": {
			"modmenu.github_sponsors": "https://github.com/sponsors/triphora",
			"modmenu.modrinth": "https://modrinth.com/mod/emcutils"
		}
	}
}
